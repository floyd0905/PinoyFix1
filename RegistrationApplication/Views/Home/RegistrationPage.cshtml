@{
    Layout = "~/Views/Shared/_RegistrationApplicationLayout.cshtml";
}
<div class="row center" ng-init="renderingNotification()">
    <!---Main Page-->

    <div class="col s12 m12 l12" style="margin-bottom:95px;">
        HEADER
    </div>

    <div class="col s2 m2 l2"> SIDE A</div>


    <!--------------------- MIDDLE --------------------->
    <div class="col s8 m8 l8">

        <div class="row">


            <div class="col s12 m12">

                <div class="z-depth-5 card blue-grey darken-1" style="border-radius: 10px; background-color: rgba(43, 39, 56, 0.8) !important; ">

                    <div class="card-content white-text">



                        <div class="row">

                            <div class="col s5 m5 l5">
                                <img />
                            </div>

                            <div class="col s7 m7 l7">
                                <div class="row">
                                    <div class="col s6 m6 l6">

                                    </div>
                                    <div class="col s6 m6 l6">

                                    </div>
                                </div>
                                <span class="card-title" style="text-align: left; font-size: 30px; margin-bottom: 2px;">Let's get you started</span>
                                <span class="card-title" style="text-align: left; font-size: 15px">Enter the details to get going</span>
                                <div class="row">
                                    <div class="input-field col s12 m4 l4">
                                        <input id="first_name" type="text" class="validate" ng-model="first_name">
                                        <label for="first_name">First Name</label>
                                    </div>
                                    <div class="input-field col s12 m4 l4">
                                        <input id="middle_name" type="text" class="validate" ng-model="middle_name">
                                        <label for="middle_name">Middle Name</label>
                                    </div>
                                    <div class="input-field col s12 m4 l4">
                                        <input id="last_name" type="text" class="validate" ng-model="last_name">
                                        <label for="last_name">Last Name</label>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="input-field col s12 m12 l12">
                                        <input id="address" type="text" class="validate" ng-model="address">
                                        <label for="address">Address</label>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="input-field col s12 m4 l4">
                                        <input id="phone_number" type="number" class="validate" ng-model="phone_number">
                                        <label for="phone_number">Phone Number</label>
                                    </div>
                                    <div class="input-field col s12 m4 l4">
                                        <input id="email" type="email" class="validate" ng-model="email">
                                        <label for="email">Email</label>
                                    </div>
                                    <div class="input-field col s12 m4 l4">
                                        <input id="password" type="password" class="validate" ng-model="password">
                                        <label for="password">Password</label>
                                    </div>
                                </div>

                                <!--------------------- BUTTONS --------------------->
                                <div class="row">
                                    <div class="col s6 m6 l6">
                                        <a class="waves-effect waves-light btn-large col s12" style="border-radius: 100px; background-color: #6e54b5;" ng-click="submitFunc()">Submit</a>
                                    </div>
                                    <div class="col s6 m6 l6">
                                        <a class="waves-effect waves-light btn-large col s12" style="border-radius: 100px; background-color: #6e54b5;" ng-click="cancelFunc()">Cancel</a>
                                    </div>
                                </div>

                            </div>
                        </div>



                        <div class="row">
                            <div class="col s6 m6 l6">

                            </div>
                            <div class="col s6 m6 l6">

                            </div>
                        </div>
                        <div class="row">
                            <div class="col s6 m6 l6">

                            </div>
                            <div class="col s6 m6 l6">

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>


    </div>

    <div class="col s2 m2 l2"> SIDE B</div>
    <div class="col s12 m12 l12"> FOOTER</div>



</div>





